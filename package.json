{
  "name": "charybdis",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npx tsc --project tsconfig.json",
    "build:watch": "npx tsc --project tsconfig.json --watch",
    "start": "node server/dist/index.js",
    "nodemon": "nodemon -q server/dist/index.js",
    "dev:server": "concurrently \"npm run build:watch\" \"npm run nodemon\"",
    "load:deps": "npm --prefix ./static/ install && npm install",
    "dev": "npm --prefix ./static/ run build && concurrently \"npm --prefix ./static/ run dev:client\" \"npm run dev:server\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "puppeteer": "^19.11.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.1",
    "@types/ws": "^8.5.4",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "typescript": "^5.0.4",
    "webpack": "^5.81.0",
    "webpack-cli": "^5.0.2",
    "ws": "^8.13.0"
  }
}
